{
	"name": "@multimonos/mm-player",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:models": "NODE_ENV=development MEDIA_URL=http://mm-media.test node --experimental-network-imports ./src/bin/make_models.js",
		"build": "npx vite build",
		"build:models": "NODE_ENV=production MEDIA_URL=https://mm-media.netlify.app node --experimental-network-imports ./src/bin/make_models.js",
		"preview": "npx vite preview",
		"test": "clear && npx playwright test",
		"test:playback": "clear && npx playwright test --grep @playback --headed --workers=1",
		"test:audiobug": "clear && npx playwright test --grep @audiobug --headed --workers=1",
		"test:unit": "npx vitest",
		"test:com": "npx cypress open --component --browser=electron"
	},
	"devDependencies": {
		"@playwright/test": "^1.30.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-netlify": "^2.0.5",
		"@sveltejs/kit": "^1.0.0",
		"@tailwindcss/typography": "^0.5.9",
		"@xstate/inspect": "^0.7.1",
		"@xstate/svelte": "^2.0.1",
		"autoprefixer": "^10.4.13",
		"cypress": "^13.6.2",
		"daisyui": "^2.51.3",
		"flatted": "^3.2.7",
		"glob": "^8.1.0",
		"iconify-icon": "^1.0.5",
		"lodash": "^4.17.21",
		"lorem-ipsum": "^2.0.8",
		"md5": "^2.3.0",
		"postcss": "^8.4.21",
		"standardized-audio-context": "^25.3.40",
		"svelte": "^3.54.0",
		"tailwindcss": "^3.2.4",
		"vite": "^4.0.0",
		"vitest": "^0.25.3",
		"xstate": "^4.35.4"
	},
	"type": "module",
	"dependencies": {
		"@portabletext/to-html": "^2.0.0",
		"@sanity/client": "^5.3.0",
		"@sanity/image-url": "^1.0.2",
		"p5": "^1.5.0",
		"pluralize-esm": "^9.0.5",
		"prismjs": "^1.29.0",
		"uuid": "^9.0.0"
	}
}
